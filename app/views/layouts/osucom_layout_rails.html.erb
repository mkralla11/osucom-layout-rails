<!DOCTYPE html>
<!--[if lt IE 7]> <html class="no-js ie6 oldie" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="no-js ie7 oldie" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="no-js ie8 oldie" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
<head>
  <meta charset="utf-8">
  
  <% content_for :default_head do %>
    <%= render(:partial=>"shared/default_head") %>
  <% end %>
  <%= content_for?(:head) ? yield(:head) : yield(:default_head) %>
  
  <%= csrf_meta_tags %>
</head>
<body>

<div id="osumc_brand_header">
  <% content_for :default_brand_header do %>
    <%= render :partial=>"shared/admin_menu" %>
    <ul id="brand">
      <li class="thissite"><a href="http://medicine.osu.edu">Ohio State's College of Medicine</a></li>
      <li><a href="http://medicine.osu.edu/faculty/ces/pages/index.aspx">Center for Education and Scholarship</a></li>
      <li><a href="http://www.osu.edu">Ohio State</a></li>
    </ul>
  <% end %>
  <%= content_for?(:brand_header) ? yield(:brand_header) : yield(:default_brand_header) %>
</div>

<div id="container">
  <div id="header" class="head" role="banner">
    <% content_for :default_header do %>
      <%= render(:partial=>"shared/default_header") %>
    <% end %>
    <%= content_for?(:header) ? yield(:header) : yield(:default_header) %>
    
    <div id="brand_bar"></div>

  </div><!-- end head -->
  <div id="topnav" class="navigation" role="navigation">
    <ul id="nav" role="menu">
      <%= render :partial=>"shared/main_menu" %>
    </ul>
  </div><!-- end topnav -->
  <div class="body">
    <div id="content" class="main" role="main">
    <%= content_for?(:content) ? yield(:content) : yield %>
    </div><!-- end main -->
  </div><!-- end body -->
</div><!-- end container -->

<div id="footer" class="footer" role="contentinfo">
  <div id="footer_contact">
    <p id="footer_address">Copyright&copy;  <a href="">
      The Ohio State University Medical Center</a><br>
      370 West 9th Avenue, Columbus, Ohio 43210
    </p>
  </div>

  <div id="footer_links">
    <ul role="menu" class="menu column_1">
    <%= render :partial=>"shared/main_menu" %>
    </ul>
    <ul class="menu column_2">
      <li><a href="http://medicalcenter.osu.edu">OSU Medical Center</a></li>
      <li><a href="http://osu.edu/">The Ohio State University</a></li>
      <li><a href="http://amp.osu.edu/">School of Allied Medical Professions</a></li>
      <li><a href="http://biomed.osu.edu/">School of Biomedical Science</a></li>
    </ul>
  </div>
</div><!-- end foot -->
<div id="modal_box">
&nbsp;
</div>

<%= debug(params) if Rails.env.development? %>

<%= yield(:footer_js) %>
<%= yield(:google_analytics) %>

<!--[if lt IE 7 ]>
  <script src="//ajax.googleapis.com/ajax/libs/chrome-frame/1.0.3/CFInstall.min.js"></script>
  <script>window.attachEvent('onload',function(){CFInstall.check({mode:'overlay'})})</script>
<![endif]-->

</body>
</html>
